@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

@section Scripts
{
    <script src="~/lib/signalr/signalr.js"></script>
    <script src="~/js/receiver.js" asp-append-version="true"></script>
    <script>
        let mainAsync = async () => {
            await MidiReceiver.initAsync("https://localhost:5000/midi");
        };
        
        (() => {
           mainAsync();
        })();
    </script>
}