@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

@section Scripts
{
    <script src="~/lib/signalr/signalr.js"></script>
    <script src="~/js/midi.js" asp-append-version="true"></script>
    <script>
        let mainAsync = async function() {
            await Midi.initAsync("https://localhost:5000/midi");
        };
        
        (function () {
           mainAsync();
        })();
    </script>
}